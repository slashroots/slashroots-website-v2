{{!< default}}
		<div id="container">
            <!-------------------Header------------------->
            <div id="header">
                <a href="/"><div id="logo"></div></a>
                <div id="menu">
                    <ul>
                        <li><a href="/keystone/signin">Signin</a></li>
                        <!-- <li>Item 2</li>
                        <li>Item 3</li>
                        <li>Item 4</li> -->
                    </ul>
                </div>
            </div>
            <!--------------------Hero-------------------->
            <div class="carousel">

                <div class="carousel-item">
                    <a href="/about">
                    <div id="hero" style="background-image: url({{posts.carousel.[0].image.url}});">
                        <div>
                            <div class="label label_idea">IDEAS</div>
                            {{posts.carousel.[0].title}}
                        </div>
                    </div>
                    </a>
                </div>

                <div class="carousel-item">
                    <a href="/post/{{posts.carousel.[1].slug}}">
                    <div id="hero" style="background-image: url({{posts.carousel.[1].image.url}});">
                        <div>
                            <div class="label label_idea">IDEAS</div>
                            {{posts.carousel.[1].title}}
                        </div>
                    </div>
                    </a>
                </div>

                <div class="carousel-item">
                    <a href="/post/{{posts.carousel.[2].slug}}">
                    <div id="hero" style="background-image: url({{posts.carousel.[2].image.url}});">
                        <div>
                            <div class="label label_idea">IDEAS</div>
                            {{posts.carousel.[2].title}}
                        </div>
                    </div>
                    </a>
                </div>
            </div>
            <!--------------------Grid-------------------->
            <div id="grid">
                <!-----------------Group1----------------->
                <div class="group" id="group_1">
                    <div class="colx1">
                        <a href="/about">
                            <div class="small_box box_top" id="box1">
                                <div>
                                    <div class="label label_idea">IDEAS</div>
                                    Who We Are
                                </div>
                            </div>
                        </a>
                        <a href="/devca">
                            <div class="small_box" id="box2">
                                <div>
                                    <div class="label label_initiative">INITIATIVE</div>
                                    Developing the Caribbean 2015 Dates Announced
                                </div>
                            </div>
                        </a>
                    </div>
                    <div class="colx2">
                        <a href="/post/{{posts.group_one_posts.[0].slug}}">
                            <div class="large_box" id="box3">
                                <div>
                                    <div class="label label_initiative">INITIATIVE</div>
                                    {{posts.group_one_posts.[0].title}}
                                </div>
                            </div>
                        </a>
                    </div>
                </div>
                <!-----------------Group2----------------->
                <div class="group" id="group_2">
                    <div class="colx2">
                        <div class="large_box" id="box4">
                            <div>
                                <div class="label label_idea">IDEAS</div>
                                Agricultural Open Data:<br>
                                Perceptions of Young Developers
                            </div>
                        </div>
                    </div>
                    <div class="colx1">
                        <div class="small_box box_top" id="box5">
                            <div>
                                <div class="label label_idea">IDEAS</div>
                                Towards a Data-driven <br>
                                Agricultural Sector
                            </div>
                        </div>
                        <div class="small_box" id="box6">
                            <div>
                                <div class="label label_idea">IDEAS</div>
                                Towards a Data-driven <br>
                                Agricultural Sector
                            </div>
                        </div>
                    </div>
                </div>
                <!-----------------Group3----------------->
                <div class="group" id="group_3">
                    {{# each data.news}}
                    <div class="colx1">
                        <div class="small_box" id="news_box1" 
                        style="background-image: url({{image.secure_url}});">
                            <div>
                                <div class="label label_news">NEWS</div>
                                {{title}}
                            </div>
                        </div>
                        <div class="news_text_box" id="news_text_box1">
                            <div class="news_content">
                                {{formatShortDate publishedDate "short_date"}}
                                {{{content.brief}}}
                            </div>
                        </div>
                    </div>
                    {{/each}}
                    {{#each data.events}}
                         <div class="colx3">
                            <div class="small_box" id="news_box3" 
                            style="background-image: url({{image.secure_url}});">
                                <div>
                                    <div class="label label_news">EVENT</div>
                                    {{title}}
                                </div>
                            </div>
                            <div class="news_text_box" id="news_text_box3">
                                <div class="news_content">
                                    {{formatShortDate publishedDatee "short_date"}}
                                    {{{content.brief}}}                                  
                                </div>
                            </div>
                        </div>                   
                      </div>
                      {{/each}}                   
                        <div style="clear:both; width:0px; height:0px;"></div>
                    </div>
                </div>
            </div>
            <!-------------------Footer------------------->
            <div id="footer"></div>
        </div>
